{
  "manifest_version": 1,
  "package": "@next-bricks/recent-history",
  "name": "recent-history",
  "bricks": [
    {
      "name": "recent-history.recent-visit",
      "properties": [
        {
          "name": "namespace",
          "description": "命名空间",
          "type": "string"
        },
        {
          "name": "capacity",
          "description": "最近访问数量",
